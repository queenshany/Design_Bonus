<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="pane" prefHeight="249.0" prefWidth="289.0" style="-fx-background-color: #cadbf7;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="boundary.EditLevelController">
   <children>
      <Label fx:id="levelNumber" layoutX="14.0" layoutY="26.0" text="Level Number">
         <font>
            <Font name="System Bold Italic" size="16.0" />
         </font>
      </Label>
      <Button fx:id="saveButton" blendMode="MULTIPLY" layoutX="121.0" layoutY="210.0" mnemonicParsing="false" onAction="#updateLevel" text=" Save " />
      <Label fx:id="maxlable" layoutX="215.0" layoutY="112.0" text="Invalid Value" visible="false" />
      <Label fx:id="winlable" layoutX="216.0" layoutY="143.0" text="Invalid Value" visible="false" />
      <Label fx:id="bonlable" layoutX="216.0" layoutY="171.0" text="Invalid Value" visible="false" />
      <Label fx:id="errorLable" layoutX="131.0" layoutY="188.0" text="Label" textFill="#e93232" visible="false" />
      <Label fx:id="lable1" layoutX="9.0" layoutY="74.0" text="Level Name" />
      <Label fx:id="label2" layoutX="14.0" layoutY="114.0" text="Difficulty" />
      <Label fx:id="label3" layoutX="14.0" layoutY="154.0" text="Block Size" />
      <TextField fx:id="levelName" editable="false" layoutX="80.0" layoutY="68.0" />
      <TextField fx:id="diffText" layoutX="80.0" layoutY="108.0" onAction="#resetLable" />
      <TextField fx:id="blockSizeText" layoutX="81.0" layoutY="148.0" onAction="#resetLable" />
      <Label fx:id="errorM" layoutX="88.0" layoutY="181.0" text="Please check your inputs" textFill="#ed3a3a" visible="false" />
   </children>
</AnchorPane>
