<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane fx:id="pane" prefHeight="249.0" prefWidth="289.0" style="-fx-background-color: #cadbf7;" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="boundary.FirstSolverController">
   <children>
      <ImageView fx:id="fireworks1" fitHeight="59.0" fitWidth="79.0" layoutX="6.0" layoutY="1.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../rsc/animated-fireworks-image.gif" />
         </image>
      </ImageView>
      <ImageView fx:id="fireworks2" fitHeight="59.0" fitWidth="79.0" layoutX="225.0" layoutY="185.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../rsc/animated-fireworks-image.gif" />
         </image>
      </ImageView>
      <Label fx:id="lable" layoutX="28.0" layoutY="120.0" prefHeight="17.0" prefWidth="250.0" text="You are the first solver of this riddle!!!" textFill="#3c60e4">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <ImageView fx:id="congPic" fitHeight="59.0" fitWidth="260.0" layoutX="14.0" layoutY="60.0" pickOnBounds="true" smooth="false">
         <image>
            <Image url="@../rsc/congPic.png" />
         </image>
      </ImageView>
      <Button fx:id="checkOutButton" blendMode="MULTIPLY" layoutX="60.0" layoutY="151.0" mnemonicParsing="false" onAction="#OpenBlocksScreen" text="Check Out Your Blocks Now" />
   </children>
</AnchorPane>
